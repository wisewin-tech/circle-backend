<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wisewin.backend.dao.InterestTypeDAO">

    <!--添加兴趣分类-->
    <insert id="addInterestType" parameterType="com.wisewin.backend.entity.bo.InterestTypeBO">
        insert into ce_interest_type (type_name,create_time,create_user_id) values (#{typeName},now(),#{createUserId})
    </insert>

    <!--修改兴趣分类-->
    <update id="updInterestType" parameterType="com.wisewin.backend.entity.bo.InterestTypeBO">
        update ce_interest_type set type_name=#{typeName},update_time=now(),update_user_id=#{updateUserId}  where id=#{id}
    </update>

    <!--查询兴趣分类-->
    <select id="getInterestTypes" resultType="com.wisewin.backend.entity.bo.InterestTypeBO">
        select * from ce_interest_type
    </select>

    <!--删除兴趣分类-->
    <delete id="delInterestType" parameterType="java.lang.Integer">
        delete from ce_interest_type where id=#{id}
    </delete>

</mapper>