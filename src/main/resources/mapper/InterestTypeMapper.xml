<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wisewin.backend.dao.InterestTypeDAO">

    <resultMap id="getInterestTypeListMap" type="com.wisewin.backend.entity.bo.InterestTypeBO">
        <result column="id" property="id"></result>
        <result column="type_name" property="typeName"></result>
        <collection property="interestBOList" ofType="com.wisewin.backend.entity.bo.InterestBO">
            <result column="interestId" property="id"></result>
            <result column="type_id" property="typeId"></result>
            <result column="interest_name" property="interestName"></result>
        </collection>
    </resultMap>
    <select id="getInterestTypeList" resultMap="getInterestTypeListMap">
       select ci_interest_type.*,i.id as interestId,i.type_id,i.interest_name
        from ci_interest_type,ci_user_interest as i
        where i.type_id=ci_interest_type.id and model_id=#{modelId}
    </select>

</mapper>